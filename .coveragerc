# Coverage.py configuration file

[run]
source = uploader_modules
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */site-packages/*

[report]
# No exclusions - testing everything for 100% total coverage
omit =

# Show lines that weren't covered
show_missing = True

# Ignore errors from missing files
ignore_errors = False

[html]
directory = tests/htmlcov
